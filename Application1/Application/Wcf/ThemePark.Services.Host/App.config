            </dependentAssembly>
        </assemblyBinding>
    </runtime>

    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="CustomServiceBehavior">
                    <serviceMetadata httpGetEnabled="False" />
                    <serviceDebug includeExceptionDetailInFaults="True" httpHelpPageEnabled="True" />
                </behavior>
            </serviceBehaviors>
            <endpointBehaviors>
                <behavior name="SessionMessageBehavior">
                    <SessionMessageElement />
                </behavior>
            </endpointBehaviors>
        </behaviors>
        <services>
            <service name="ThemePark.Services.Implementation.TestJobService" behaviorConfiguration="CustomServiceBehavior">
                <host>
                    <baseAddresses>
                        <add baseAddress="net.tcp://localhost:8100/WcfServices/" />
                    </baseAddresses>
                </host>
                <endpoint address="TestJobService" binding="netTcpBinding" bindingConfiguration="" behaviorConfiguration="SessionMessageBehavior" name="TestJobService" contract="ThemePark.Services.Interface.ITestJobService" />
                <endpoint address="mex" contract="IMetadataExchange" binding="mexTcpBinding" />
                <!--<host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8100/WcfServices/" />
                    </baseAddresses>
                </host>
                <endpoint address="TestJobService" binding="wsHttpBinding" bindingConfiguration="" contract="ThemePark.Services.Interface.ITestJobService" />
                <endpoint address="mex" contract="IMetadataExchange" binding="mexHttpBinding" />-->
            </service>
        </services>
        <extensions>
            <behaviorExtensions>
                <add name="SessionMessageElement" type="ThemePark.Infrastructure.Services.SessionPropagationBehaviorElement, ThemePark.Infrastructure" />
            </behaviorExtensions>
        </extensions>
    </system.serviceModel>
